{
  "name": "flight-delay-api",
  "version": "1.0.0",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "start:all": "concurrently \"npm run dev\" \"npm run client:dev\"",
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "lint": "eslint .",
    "client:dev": "vite",
    "client:build": "tsc -b client && vite build --cwd client",
    "client:lint": "eslint client",
    "client:preview": "vite preview --cwd client"
  },
  "dependencies": {
    "fastify": "^4.25.2",
    "@sinclair/typebox": "^0.30.0",
    "@fastify/swagger": "^8.10.0",
    "@fastify/swagger-ui": "^2.0.0",
    "@fastify/cors": "^8.4.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@mui/material": "^5.15.15",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "9.1.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "@types/node": "^22.0.0"
  }
}
